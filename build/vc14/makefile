	@-if exist $(TARG_SCRT_ACE_MT_R) $(RM) $(TARG_SCRT_ACE_MT_R)
	@-if exist $(TARG_SCRT_ACE_WS_DLL_D) $(RM) $(TARG_SCRT_ACE_WS_DLL_D)
	@-if exist $(TARG_SCRT_ACE_WS_DLL_R) $(RM) $(TARG_SCRT_ACE_WS_DLL_R)
	@-if exist $(TARG_SCRT_ACE_WS_MT_D) $(RM) $(TARG_SCRT_ACE_WS_MT_D)
	@-if exist $(TARG_SCRT_ACE_WS_MT_R) $(RM) $(TARG_SCRT_ACE_WS_MT_R)
!endif # ACE

clean.unittest.std:
	@echo Cleaning standard unittests ...

!ifdef ACE
clean.unittest.ace:
	@echo Cleaning ACE unittests ...
!endif # ACE


